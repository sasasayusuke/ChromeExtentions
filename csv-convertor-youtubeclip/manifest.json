{
  "manifest_version": 3,
  "name": "YoutubeClip",
  "version": "1.1.49",
  "description": "Youtubeの中身をいい感じにclipします。",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/リスのアイコン16.png",
      "48": "icons/リスのアイコン48.png",
      "128": "icons/リスのアイコン128.png"
    }
  },
  "icons": {
    "16": "icons/リスのアイコン16.png",
    "48": "icons/リスのアイコン48.png",
    "128": "icons/リスのアイコン128.png"
  },
  "host_permissions": []
}
