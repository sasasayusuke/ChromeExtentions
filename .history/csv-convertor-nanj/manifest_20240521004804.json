      "16": "icons/バットアイコン16.png",
      "48": "icons/バットアイコン48.png",
      "128": "icons/バットアイコン128.png"
    }
  },
  "icons": {
    "16": "icons/バットアイコン16.png",
    "48": "icons/バットアイコン48.png",
    "128": "icons/バットアイコン128.png"
  },
  "host_permissions": []
}
